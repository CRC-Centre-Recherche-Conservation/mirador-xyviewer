{
  "name": "mirador-xyviewer",
  "version": "1.0.0",
  "description": "Mirador 4 plugin for scientific IIIF annotations with spectra visualization",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --config demo/vite.config.ts",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "peerDependencies": {
    "mirador": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-redux": "^8.0.0 || ^9.0.0",
    "@mui/material": "^7.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "dependencies": {
    "mirador-image-tools": "^1.0.0",
    "papaparse": "^5.4.1",
    "plotly.js": "^3.3.1",
    "react-i18next": "^15.x.x",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@redux-devtools/extension": "^3.3.0",
    "redux": "^5.0.0",
    "react-redux": "^9.1.0",
    "mirador": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@mui/material": "^7.0.0",
    "@mui/icons-material": "^7.0.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mirador-xyviewer"
  },
  "keywords": [
    "mirador",
    "iiif",
    "plugin",
    "annotations",
    "spectra",
    "plotly",
    "scientific",
    "xrf",
    "raman",
